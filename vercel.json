{
  "builds": [
    {
      "src": "musicplayer/wsgi.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/staticfiles/$1"
    },
    {
      "src": "/media/(.*)",
      "dest": "/media/$1"
    },
    {
      "src": "/(.*)",
      "dest": "musicplayer/wsgi.py"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "musicplayer.settings_prod"
  }
}
